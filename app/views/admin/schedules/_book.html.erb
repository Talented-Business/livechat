<span class="bookable_span" title="Book" >bookable
  <% 
    unless defined? new_schedule 
      new_schedule = @schedule
      id = @element_id
    end
  %>  
  <%= form_for [:admin, new_schedule], :remote=>true do |f| %>
    <input type="hidden" name="element_id" value="<%= id %>" />
    <%= f.hidden_field :thedate %>
    <%= f.hidden_field :number %>
    <%= f.hidden_field :operator_id %>
  <% end %>
</span>    
